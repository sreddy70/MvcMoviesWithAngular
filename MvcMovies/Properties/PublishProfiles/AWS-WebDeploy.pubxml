    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>54.154.50.172</MSDeployServiceURL>
    <DeployIisAppPath>MvcMovies</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>False</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>administrator</UserName>
    <_SavePWD>True</_SavePWD>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="DefaultConnection" Order="1" Enabled="False">
          <Destination Path="" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="MvcMovies.Models.ApplicationDbContext, MvcMovies" Origin="Convention" />
          </Object>
        </ObjectGroup>
        <ObjectGroup Name="MySqlLocalDb" Order="2" Enabled="False">
          <Destination Path="" />
          <Object Type="DbDacFx">
            <PreSource Path="server=localhost;user id=root;database=mvcmovies;port=3306;password=@lt12345;pooling=False" includeData="False" />
            <Source Path="$(IntermediateOutputPath)AutoScripts\MySqlLocalDb_IncrementalSchemaOnly.dacpac" dacpacAction="Deploy" />
          </Object>
          <UpdateFrom Type="Web.Config">
            <Source MatchValue="server=localhost;user=root;database=mvcmovies;port=3306;password= @lt12345;pooling=false" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
          </UpdateFrom>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)DefaultConnection-Web.config Connection String">
