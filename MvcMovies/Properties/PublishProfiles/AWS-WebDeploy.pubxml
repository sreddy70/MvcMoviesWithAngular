    </MSDeployParameterValue>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)MySqlLocalDb-Web.config Connection String" />
  </ItemGroup>
  <ItemGroup>
    <_ConnectionStringsToInsert Include="DefaultConnection" />
  </ItemGroup>
